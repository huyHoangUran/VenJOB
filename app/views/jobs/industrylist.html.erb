<div>
    
    <div class="p-5">
        <h4>Viet Nam</h4>
        <div class="row p-5 ">
            <h3>Top Cities</h3>
            <% @listIndustries.each.with_index do |item| %>
              <div class="col-md-3  d-flex justify-content-center pb-3">
                <div class="single_company border mx-auto" style="width: 70%;">
                  <a href="jobs.html">
                    <h3 class="bold"><%= item.name %></h3>
                  </a>
                  <p><%= item.job_count %></p>
